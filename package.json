{
  "name": "n8n-nodes-synology-suite",
  "version": "0.4.1",
  "description": "n8n community nodes for Synology DSM (Drive, Office/Sheets, MailPlus)",
  "license": "MIT",
  "author": "Maxime Olivier",
  "homepage": "https://github.com/darkbowfr/n8n-nodes-synology-suite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkbowfr/n8n-nodes-synology-suite.git"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "synology",
    "dsm",
    "drive",
    "mailplus",
    "office",
    "spreadsheet"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint nodes credentials package.json",
    "smoke": "bash ./scripts/smoke-check.sh",
    "probe:actions": "python3 ./scripts/probe-actions.py",
    "probe:actions:destructive": "python3 ./scripts/probe-actions.py --allow-destructive",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "eslint": "^8.57.1",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.82.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SynologyDsmApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SynologyApi/SynologyApi.node.js",
      "dist/nodes/SynologyDrive/SynologyDrive.node.js",
      "dist/nodes/SynologyOffice/SynologyOffice.node.js",
      "dist/nodes/SynologyMailPlus/SynologyMailPlus.node.js",
      "dist/nodes/SynologyMailPlusTrigger/SynologyMailPlusTrigger.node.js"
    ]
  }
}
